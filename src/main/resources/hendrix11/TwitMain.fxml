<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.VBox?>
<ScrollPane xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="hendrix11.TwitMain">
    <content>
        <VBox fx:id="list">
            <TabPane>
                <Tab closable="false" text="Following">
                    <fx:include fx:id="followingTable" source="UserInfo.fxml"/>
                </Tab>
                <Tab closable="false" text="Followers">
                    <fx:include fx:id="followerTable" source="UserInfo.fxml"/>
                </Tab>
                <Tab closable="false" text="Tweet Scheduler">

                </Tab>
                <Tab closable="false" text="Retweeter">

                </Tab>
            </TabPane>
        </VBox>
    </content>
</ScrollPane>